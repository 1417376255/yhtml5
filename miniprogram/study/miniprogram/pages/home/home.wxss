/* pages/home/home.wxss */

@import "../../style/guide.wxss";

.notice {
  box-sizing: border-box;
  position:fixed;
  z-index:100;
  width:100%;
  text-align:center;
}

.notice .y-body {
  background-color:#fff;
  margin:20px;
  height: 30px;
  padding-top: 4px;
}

.service {
  position:fixed;
  z-index:100;
  width:40px;
  height:40px;
  top:80px;
  right:10px;
  background-color:#fff;
  border-radius:50%;
  line-height:40px;
  font-size:14px;
  text-align:center;
}

.tabbar{
  position:fixed;
  z-index:100;
  height:40px;
  /* border-top:solid #D8D8D8 1px; */
  background-color: #fff;
  bottom:0;
  width:100%;
  display: flex;
  text-align: center;
}

.tabbar-item{
  flex: 1;
  padding-top:8px;
}
